{
	"root": true,
	"extends": [
		"wikimedia/client-es5",
		"wikimedia/jquery",
		"wikimedia/mediawiki",
		"wikimedia/jsduck"
	],
	"globals": {
		"Chart": "readonly"
	},
	"rules": {
		"max-len": "off",
		"no-use-before-define": "off",
		"no-jquery/no-animate-toggle": "warn",
		"no-jquery/no-class-state": "off",
		"no-jquery/no-global-selector": "off",
		"no-jquery/no-sizzle": "warn",
		"no-jquery/no-visibility": "error",
		"compat/compat": "warn"
	}
}
